{
  "presets": [
    [
      "@babel/preset-env",
      { "targets": { "browsers": "last 2 versions" } }
    ],
    "@babel/preset-typescript",
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-class-properties",
      { "loose": true }
    ],
    ["react-intl",
      {
        "messagesDir": "./tmp/extracted-messages/"
      }]
  ]
}